[32m+[m[32mfrom fastapi import FastAPI[m
[32m+[m[32mfrom fastapi.middleware.cors import CORSMiddleware[m
[32m+[m
[32m+[m[32mfrom routes import transactions, indicators[m
[32m+[m
[32m+[m[32mapp = FastAPI()[m
[32m+[m
[32m+[m[32m# Enable CORS (React frontend needs this)[m
[32m+[m[32mapp.add_middleware([m
[32m+[m[32m    CORSMiddleware,[m
[32m+[m[32m    allow_origins=["http://localhost:3000"],  # React dev server[m
[32m+[m[32m    allow_credentials=True,[m
[32m+[m[32m    allow_methods=["*"],[m
[32m+[m[32m    allow_headers=["*"],[m
[32m+[m[32m)[m
[32m+[m
[32m+[m[32m# Register routers[m
[32m+[m[32mapp.include_router(transactions.router, prefix="/transactions", tags=["Transactions"])[m
[32m+[m[32mapp.include_router(indicators.router, prefix="/indicators", tags=["Indicators"])[m
[32m+[m
[32m+[m[32m@app.get("/")[m
[32m+[m[32mdef root():[m
[32m+[m[32m    return {"message": "Expense Tracker API running"}[m
